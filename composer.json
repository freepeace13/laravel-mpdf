{
    "name": "freepeace/laravel-mpdf",
    "description": "Laravel package wrapper for mpdf",
    "keywords": [
        "freepeace",
        "laravel-mpdf"
    ],
    "homepage": "https://github.com/freepeace/laravel-mpdf",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kin Basco",
            "email": "freepeace13@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "illuminate/support": "^9.0",
        "mpdf/mpdf": "^8.2.5"
    },
    "require-dev": {
        "laravel/pint": "^1.20",
        "orchestra/canvas": "^7.12",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.23",
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelMpdf\\Providers\\LaravelMpdfServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "LaravelMpdf\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelMpdf\\Tests\\": "tests"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint --config pint.json",
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}